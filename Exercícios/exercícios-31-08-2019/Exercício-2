#include <stdio.h>

int main()
{
    char frase[100];
    int i = 0, contador = 0;
    
    
    scanf("%[^\n]%*c",frase);

    
    while(frase[i] != '\0'){
        if(frase[i] == ' '){
            contador++;
        }
        i++;
    }

    printf("%d vezes que o espa√ßo aparece.",contador);
    return 0;
}
